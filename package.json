{
  "name": "sails-mysql-atomic",
  "version": "1.5.0",
  "description": "Helper service to facilitate the usage of mysql transactions",
  "main": "index.js",
  "sails": {
    "isHook": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BesLogic/sails-mysql-atomic.git"
  },
  "scripts": {
    "test": "set NODE_ENV=test&&mocha -R spec",
    "test-debug": "set NODE_ENV=test&&mocha --debug -R spec",
    "test-cover": "set NODE_ENV=test&&istanbul cover node_modules/mocha/bin/_mocha -- test/**/*.spec.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "transaction-tarball": "true",
  "keywords": [
    "sails",
    "waterline",
    "orm",
    "mysql",
    "transactions",
    "sails-mysql-atomic",
    "atomic"
  ],
  "author": "Samuel Poirier",
  "license": "ISC",
  "devDependencies": {
    "chai": "~3.5.0",
    "eslint": "~4.1.0",
    "istanbul": "~0.4.5",
    "mocha": "~3.3.0",
    "newman": "~3.5.0",
    "should": "~11.2.0",
    "sinon": "~2.1.0",
    "coveralls": "~2.13.0"
  },
  "dependencies": {
    "lodash": "~4.17.2",
    "sails": "~0.12.13",
    "sails-mysql": "~0.11.5",
    "winston": "~2.3.0"
  },
  "engines": {
    "node": ">=6.9.2"
  }
}
